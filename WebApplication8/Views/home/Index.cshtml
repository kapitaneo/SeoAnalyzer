@model string[]
@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<div>
    @using (Ajax.BeginForm("Textsearch", new AjaxOptions { UpdateTargetId = "result" }))
    {
        <table>
          <tr>
              <td>
                  @Html.Label("Please, write url")
              </td>
          </tr>
          <tr>
              <td>
                  @Html.TextArea("text")       
                  <input type="submit" value="Enter" />
              </td>
              <td>
                  @foreach (string value in Model)
                  {
                     <label><input type="checkbox" id="" name="checkedValues" value="@value">@value</label> <br />
                  }

              </td>
        </tr>
        </table>
    }
    <div id="result"></div>
</div>
@Scripts.Render("~/scripts/jquery-1.8.0.js")
@Scripts.Render("~/scripts/jquery.unobtrusive-ajax.js")